    matrix.drawPixel(j, i, pgm_read_dword(&(m3[k])));
    else if(n==1)
    matrix.drawPixel(j, i, pgm_read_dword(&(m4[k])));
    else if(n==2)
    matrix.drawPixel(j, i, pgm_read_dword(&(m1[k])));
    else if(n==3)
    matrix.drawPixel(j, i, pgm_read_dword(&(m5[k])));
    else if(n==4)
    matrix.drawPixel(j, i, pgm_read_dword(&(m3[k])));


    
